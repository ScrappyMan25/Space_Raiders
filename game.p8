--initialization
function _init()
	hi = 0 -- high score variable
	gamestate = {{init_menu, update_menu, draw_menu}, {initgame, updategame, drawgame},{init_end, update_end, draw_end}}
	state = 1
	gamestate[state][1]()
	counter = 0 -- frame counter
end

function _update60()
	gamestate[state][2]()
	counter += 1
end

function _draw()
	cls()
	gamestate[state][3]()
end
